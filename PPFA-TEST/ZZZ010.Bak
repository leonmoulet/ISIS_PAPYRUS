  /*************************************************************
                                                            
  ELIXIR PAGE DEFINITION CIR010                             
                                                            
  NAME:           CIR010                                    
  CREATED:        ENE-2007                                  
                                                            
  COMMENTS:                                                 
  UTILIZA 2 OVERLAYS INVOCADOS DINAMICAMENTE POR LOS DATOS  
  VARIABLES: CIR002 Y CIR003, ESCRITOS EN DUPLEX.           
  EL LISTADO COMIENZA CON UN CIR002 Y UN NUMERO NO FIJO     
  DE CIR003.                                                
  CUANDO VENGA UN NUEVO CIR002, HAY QUE PASAR A HOJA NUEVA  
                                                            
  16-05-2014: SE CAMBIAN LOS NOMBRES DE LOS OVERLAYS,       
              CIR010 Y CIR011 EN LUGAR DE CIR002 Y CIR003.  
                                                            
  02-09-2015: SE AMPLIAN TAMAÑOS DE CAMPOS A 100 CHARS.     
  19-02-2019: SE ELIMINA CAMPO FECHA.                       
  *************************************************************/
SETUNITS  1.00 CM  1.00 CM LINESP  0.42 CM ;
FORMDEF ZZZ010
      DUPLEX NORMAL
      BIN 1
      JOG NO
      OFFSET  1.00 PELS   1.00 PELS   1.00 PELS   1.00 PELS 
      PRESENT PORTRAIT
      DIRECTION ACROSS
      CUTSHEET NO
      REPLACE YES;

 COPYGROUP ZZZ010
      DUPLEX NORMAL
      PRESENT PORTRAIT
      DIRECTION ACROSS
      CONSTANT BACK;
  OVERLAY ZZZ011 ZZZ011 NORASTER;
  OVERLAY ZZZ010 ZZZ010 NORASTER;
  OVERLAY ZZZ012 ZZZ012 NORASTER;
  SUBGROUP
      FRONT
      OVERLAY
       ZZZ010;
  SUBGROUP
      BACK
      OVERLAY
       ZZZ012;

 COPYGROUP ZZZ011
      DUPLEX NORMAL
      PRESENT PORTRAIT
      DIRECTION ACROSS;
  SUBGROUP
      BOTH;


SETUNITS  1.00 CM  1.00 CM LINESP  0.42 CM ;
PAGEDEF ZZZ010
      WIDTH  20.99  
      HEIGHT  29.69  
      LINEONE  0.00   0.00  
      DIRECTION ACROSS
      REPLACE YES;
